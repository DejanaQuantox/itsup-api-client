{
    "name":              "itsup/itsup-api-client",
    "description":       "Guzzle Client for api.itsup.com",
    "type":              "library",
    "require":           {
        "guzzlehttp/guzzle":           "^6.0",
        "symfony/options-resolver":    "^2.7|^3.0",
        "league/fractal":              "^0.13.0",
        "doctrine/annotations":        "^1.2",
        "doctrine/cache":              "^1.6",
        "symfony/expression-language": "^2.7|^3.0",
        "symfony/form":                "^2.7|^3.0"
    },
    "license":           "Proprietary",
    "suggest": {
        "cache/cache": "Required for caching responses from the API"
    },
    "authors":           [
        {
            "name":  "Cyril LEGRAND",
            "email": "cyril@sctr.net"
        }
    ],
    "autoload":          {
        "psr-4": {
            "Itsup\\Api\\": "src"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "symfony/var-dumper": "^3.0"
    }
}
